<?xml version="1.0" encoding="utf-8"?>
<s:View xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark" title="Quiz Result">
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	
	<s:navigationContent>
		<s:Button label="Back" click="navigator.popView();"/>
	</s:navigationContent>
	<s:actionContent>
		<s:Button label="Home" click="navigator.popToFirstView();"/>
	</s:actionContent>
	<s:layout>
		<s:VerticalLayout paddingTop="20" paddingLeft="10" paddingRight="10" horizontalAlign="center"/>
	</s:layout>
	<s:Spacer width="10" height="37"/>
	<s:Label width="185" height="38" fontSize="36" text="You Scored"/>
	<s:Spacer width="10" height="37"/>
	<s:Label fontSize="40" text="{data.score}"/>
</s:View>
